{
    "name": "thecodebunny/magento2-api",
    "description": "Rest API client for Magento 2 generated by swagger-codegen.",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api",
        "magento2"
    ],
    "homepage": "http://hemangvyas.com",
    "license": "Proprietary",
    "authors": [{
        "name": "Hemang Vyas (TheCodeBunny)",
        "homepage": "https://github.com/thecodebunny/TcbMagento2Api"
    }],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "TheCodeBunny\\Magento2Api\\": "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "TheCodeBunny\\Magento2Api\\": "test/" }
    }
}